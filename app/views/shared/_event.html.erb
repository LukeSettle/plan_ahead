<div class="row event">
  <div class="col-xs-3">
    <%= link_to event.title, event_path(event) %>
  </div>
  <div class="col-xs-3">
    <%= event.kind %>
  </div>
  <div class="col-xs-3">
    <%= format_datetime event.starts_at %>
  </div>
  <div class="col-xs-3">
    <%= format_datetime event.ends_at %>
  </div>
</div>
<div class="row event-link">
  <div class="col-xs-12">
    <%= Onebox.preview(event.link).to_s %>
  </div>
</div>
